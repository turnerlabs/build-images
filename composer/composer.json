  "require-dev": {
    "drupal/drupal-extension": "v3.4.1",
    "drupal/console": "^1.8",
    "phpmd/phpmd": "2.5.0",
    "squizlabs/php_codesniffer": "2.9.1",
    "phpunit/phpunit": "6.5.13",
    "phpunit/phpcov": "@stable",
    "drupal/coder": "8.2.12",
    "drupal/devel": "1.2.0",
    "rregeer/phpunit-coverage-check": "0.1.6"
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "post-install-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ]
  },
  "extra": {
    "composer-exit-on-patch-failure": true,
    "installer-paths": {
      "build/html/core": [
        "type:drupal-core"
      ],
      "build/html/modules/custom/{$name}": [
        "type:drupal-module-custom"
      ],
      "build/html/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "build/html/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "build/html/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "build/html/libraries/{$name}": [
        "type:drupal-library"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "patches": {
      "drupal/workbench_moderation_actions": {
        "Correctly set revision UUID": "https://www.drupal.org/files/issues/2893699-6-access_and_set_revision_uid_log.patch",
        "Reset core actions on uninstall": "https://www.drupal.org/files/issues/restore_publish_actions_after_uninstall-2891765-10.patch"
      },
      "drupal/migrate_source_json": {
        "[2660670]": "https://www.drupal.org/files/issues/migrate_source_json-php_7_seg_fault-2660670-3_0.patch"
      },
      "drupal/zurb_foundation": {
        "[2701371]": "https://www.drupal.org/files/issues/zurb_foundation_revision_view_2701371_2_D8.patch"
      },
      "drupal/redis": {
        "[2765895]": "https://www.drupal.org/files/issues/redis-n2765895-16.patch",
        "[2876099]": "https://www.drupal.org/files/issues/2876099_1_redis_sessions.patch",
        "Remove assert for cache tags": "patches/redis_assert.patch",
        "Use persistent connection": "patches/redis_persist.patch"
      },
      "drupal/workbench_moderation": {
        "Fix PHP 7.2 error": "https://www.drupal.org/files/issues/2018-04-16/workbench_moderation_incompatible-declaration_2952717_4.patch"
      },
      "monolog/monolog": {
        "Fix recursion error": "https://patch-diff.githubusercontent.com/raw/Seldaek/monolog/pull/1050.patch"
      },
      "drupal/paragraphs_library": {
        "fix for reusable paragraphs": "https://www.drupal.org/files/issues/convert_a_normal-2847053-52.patch"
      },
      "drupal/tablefield": {
        "fix for reusable paragraphs": "https://www.drupal.org/files/issues/2018-10-23/2922803_24.patch"
      }
    }
  },
  "config": {
    "prestissimo": {
      "verbose": false,
      "privatePackages": []
    },
    "platform": {
      "php": "7.2.16"
    }
  }
}
